{
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@setup/*": ["./setup/*"],
      "@featureMap/*": ["./feature-map/*"],
      "@pages/*": ["./src/pages/*"],
      "@services/*": ["./src/services/*"]
    }
  },
  "exclude": ["node_modules/**"]
}
